age = resData\r\n      // eslint-disable-next-line no-implied-eval\r\n\r\n      this.$message({\r\n        message: '上传成功',\r\n        type: 'success',\r\n        duration: 2000\r\n      })\r\n      this.$router.push('/video')\r\n    },\r\n    close() {\r\n      this.imagecropperShow = false\r\n    },\r\n    onSubmit() {\r\n      var vm = this\r\n      this.axios({\r\n        method: 'POST',\r\n        data: vm.video,\r\n        url: 'http://localhost:8081/admin/editVideo'\r\n      }).then(function(resp) {\r\n        vm.$message({\r\n          message: '修改成功',\r\n          type: 'success'\r\n        })\r\n        vm.$router.push('/video')\r\n      })\r\n    },\r\n    onCancel() {\r\n      this.$message({\r\n        message: '返回中!',\r\n        type: 'success'\r\n      })\r\n      this.$router.push('/user')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.line {\r\n  text-align: center;\r\n}\r\n\r\n.el-input {\r\n  width: 200px;\r\n}\r\n\r\n.avatar {\r\n  width: 120px;\r\n  height: 200px;\r\n}\r\n</style>\r\n\r\n"]}]}