,KApBA,CAoBA;QACA;UACAF,YADA;UAEAC;QAFA;MAIA,CAzBA;IA0BA,CAvDA;IAwDA;IACAE,SAzDA,uBAyDA;MACA;MACA;QACAd,aADA;QAEAC;MAFA,GAGAC,IAHA,CAGA;QACAC;QACAY;MACA,CANA;IAOA;EAlEA;AAvBA","names":["components","Pagination","data","videoTypeName","total","listQuery","page","limit","list","created","methods","onSubmit","method","url","then","vm","getList","addCategory","editCategory","id","delCategory","confirmButtonText","cancelButtonText","type","message","catch","fetchData","console"],"sourceRoot":"src/views/category","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\r\n      <el-form-item label=\"分类名称\">\r\n        <el-input v-model=\"videoTypeName\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"warning\" @click=\"onSubmit\">查询</el-button>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"success\" @click=\"addCategory\">添加分类</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-table\r\n      :data=\"list\"\r\n      border\r\n      fit\r\n      highlight-current-row\r\n    >\r\n      <el-table-column align=\"center\" label=\"序号\" width=\"95\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.$index + 1 }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column align=\"center\" label=\"类型\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.typeName }}\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"230\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"mini\" @click=\"editCategory(scope.row.videotypeId)\">\r\n            编辑\r\n          </el-button>\r\n          <el-button type=\"danger\" size=\"mini\" @click=\"delCategory(scope.row.videotypeId)\">\r\n            删除\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <pagination v-show=\"total>0\" :total=\"total\" :page.sync=\"listQuery.page\" :limit.sync=\"listQuery.limit\"\r\n                @pagination=\"getList\"\r\n    />\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport Pagination from '@/components/Pagination' // secondary package based on el-pagination\r\nexport default {\r\n  components: { Pagination },\r\n  data() {\r\n    return {\r\n      videoTypeName: '',\r\n      total: 0,\r\n      listQuery: {\r\n        page: 1,\r\n        limit: 5\r\n      },\r\n      list: [\r\n        {\r\n          'videotypeId': 1,\r\n          'typeName': '影视'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  created() {\r\n    // this.fetchData()\r\n    this.getList()\r\n  },\r\n\r\n  methods: {\r\n    onSubmit() {\r\n      var vm = this\r\n      this.axios({\r\n        method: 'GET',\r\n        url: 'http://localhost:8081/admin/searchVideoType?pageNum=' + vm.listQuery.page + '&pageSize=' + vm.listQuery.limit + '&typeName=' + vm.videoTypeName\r\n      }).then(function(resp) {\r\n        vm.total = resp.data.total // 讲pageInfo中的total放到vm的total\r\n        vm.list = resp.data.list\r\n      })\r\n    },\r\n    getList() {\r\n      var vm = this\r\n      this.axios({\r\n        method: 'GET',\r\n        url: 'http://localhost:8081/admin/categoryPageInfo?pageNum=' + vm.listQuery.page + '&pageSize=' + vm.listQuery.limit\r\n      }).then(function(resp) {\r\n        vm.total = resp.data.total // 讲pageInfo中的total放到vm的total\r\n        vm.list = resp.data.list\r\n      })\r\n    },\r\n    addCategory() {\r\n      this.$router.push('/addcategory/index')\r\n    },\r\n    editCategory(id) {\r\n      this.$router.push('/editcategory/index/' + id)\r\n    },\r\n    delCategory(id) {\r\n      var vm = this\r\n      this.$confirm('此操作将删除分类, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        vm.axios({\r\n          method: 'GET',\r\n          url: 'http://localhost:8081/admin/deleteCategory/' + id\r\n        }).then(function(resp) {\r\n          if (resp.data === 'success') {\r\n            vm.$message({\r\n              message: '删除成功',\r\n              type: 'success'\r\n            })\r\n            vm.getList()\r\n          }\r\n          // eslint-disable-next-line handle-callback-err\r\n        }).catch(function(error) {\r\n          vm.$message.error('删除失败')\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        })\r\n      })\r\n    },\r\n    // 获取用户列表\r\n    fetchData() {\r\n      var vm = this\r\n      this.axios({\r\n        method: 'GET',\r\n        url: 'http://localhost:8081/admin/list'\r\n      }).then(function(resp) {\r\n        vm.list = resp.data\r\n        console.log(resp.data)\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}